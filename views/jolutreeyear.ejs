<%- include('partials/header',{user:currentuser}); -%>

<div class="jolutree-year-page">
    <div style="width: 100%; margin-top: 10px;">
        <div class="jolutree-btn-grp">
            <div class="jolutree-btn-grp__item"><a href="./*">All</a></div>
            <div class="jolutree-btn-grp__item"><a href="./me">ME</a></div>
            <div class="jolutree-btn-grp__item"><a href="./ce">CE</a></div>
            <div class="jolutree-btn-grp__item"><a href="./ee">EE</a></div>
            <div class="jolutree-btn-grp__item"><a href="./cse">CSE</a></div>
            <div class="jolutree-btn-grp__item"><a href="./it">IT</a></div>
            <div class="jolutree-btn-grp__item"><a href="./ece">ECE</a></div>
        </div>
        <% if(people.length===0){ %>
          <h1 style="text-align:center;padding-top:50px">No User Found</h1>
          <% }%>
        <div class="jolutree-user-profiles">


          <% people.forEach(function(user){ %>
            <div class="jolutree-user-profile-container">
                <div class="jolutree-user-profile-img-container">
                    <div class="jolutree-user-profile-img-border">
                        <img src= "/uploads/<%=user.dp%>" alt="Profile Image">
                    </div>
                </div>
                <hr class="jolutree-user-profile-container-divider">
                <div class="jolutree-user-profile-other-details">
                    <div>
                        <h6><strong><%=user.name%></strong></h6>
                         <p><strong>Current Position:</strong> <%=user.currorg%></p>
                        <p><strong>Previous Position:</strong><%=user.prevorg%></p>
                        <p></p>
                    </div>
                </div>
                <hr class="jolutree-user-profile-container-divider">
                <div class="jolutree-user-profile-social-media">
                    <ul>
                         <li><a href="https://www.linkedin.com/in/souviksarkhel/" target="_blank"><i class='bx bxl-linkedin-square'></i></a></li>
                        <li><a href="https://github.com/souviksarkhel621" target="_blank"><i class='bx bxl-github'></i></a></li>
                        <li><a href="https://www.instagram.com/the_hellslyte_/" target="_blank"><i class='bx bxl-instagram'></i></a></li>
                    </ul>
                </div>
            </div>
            <% })%>

        </div>
    </div>
</div>

<%- include('partials/footer'); -%>
