                          <div style="display: flex; align-items: center;">

                            <div class="document-type__img">
                              <!-- pdf -->
                              <% if(post.postfile.substr(post.postfile.length - 4)==".pdf"){%>
                                <!-- <h1>Case1</h1> -->
